{
  "expo": {
    "name": "travel-app",
    "slug": "travel-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon-2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/app-icon-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dmx214.wanderly",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon-2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dmx214.wanderly"
    },
    "owner": "dmx214",
    "plugins": [
      "expo-secure-store",
      "expo-system-ui"
    ],
    "extra": {
      "eas": {
        "projectId": "01fc402b-7681-4abb-a049-6c4f6dc1bb1c"
      }
    }
  }
}
